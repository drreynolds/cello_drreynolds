#!/bin/csh -f

set n      = 32

foreach offset (0 1)

   foreach serial (0 1)

      foreach level (1 2 3 4)

         echo "$level $offset $serial"
         run $n 1 1 1 $level $offset $serial
         run $n 2 1 1 $level $offset $serial
         run $n 1 2 1 $level $offset $serial
         run $n 1 1 2 $level $offset $serial
         run $n 1 2 2 $level $offset $serial
         run $n 2 1 2 $level $offset $serial
         run $n 2 2 1 $level $offset $serial

         rm -rf N32*

      end

   end

end
