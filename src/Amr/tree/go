#/bin/csh -f

set run = 0

set colormap_dir = /usr/share/h5utils/colormaps/

set color = gray

if ($run == 1) then
  make dep
  make clean
  make
  test_tree
  echo "done"
endif


foreach tree (4 16)
    foreach type (0 1 2 3 4 5)
      echo "tree$tree-$type.hdf5"
      h5topng -c $colormap_dir/$color tree$tree-$type.hdf5
    end
end



