#/bin/csh -f

set run = 0

set colormap_dir = /usr/share/h5utils/colormaps/

set color = gray

if ($run == 1) then
  make dep
  make clean
  make
  test_tree
  echo "done"
endif


foreach tree (2 4 8)
  foreach full (0 1)
    foreach type (0 1 2)
      set file = "tree$tree-f$full-$type.hdf5"
      echo "$file"
      h5topng -c $colormap_dir/$color $file
    end
  end
end



