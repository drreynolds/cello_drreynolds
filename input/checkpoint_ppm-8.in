# Problem: 2D Implosion problem
# Author:  James Bordner (jobordner@ucsd.edu)

include "input/ppm.incl"

Mesh { root_blocks    = [2,4]; }

Output {

  list = ["density","checkpoint"];

  density {
     name = ["checkpoint_ppm-8-%06d.png", "cycle"];
  };

  checkpoint {

     type  = "checkpoint";
     dir   = ["checkpoint_ppm-8-%d","cycle"];
     schedule { var = "cycle"; type ="list"; value=[100,200,300];};
  }
}
