      };

      te {
    
        axis = "z";    
	type     = "image";
        name     = ["sedov-te-%03d.png","cycle"];
        field_list = ["total_energy"];
#	image_ghost = true;
	schedule = ["cycle","interval", 100];
#	image_size = [1200,1200];
