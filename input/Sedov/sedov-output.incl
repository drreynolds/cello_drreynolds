         name = ["sedov-mesh-%03d.png", "cycle"]; 
	type     = "image";
	image_type  = "mesh";
	image_reduce_type = "max";
	image_block_size  = 5;
#	image_size = [513,513];
#	image_size = [1201,1201];
	schedule = ["cycle","interval", 100];
        colormap = [1.00, 1.00, 1.00,  
                    0.00, 1.00, 0.00, 
